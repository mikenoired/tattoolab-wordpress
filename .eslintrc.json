{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-unresolved": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    },
    "ignorePatterns": [
        "node_modules", "/dist/**"
    ]
}
